{
  "compilerOptions": {
    "target": "es6",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "module": "ES6"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
